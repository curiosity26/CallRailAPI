{
  "name": "curiosity26/callrailapi",
  "description": "An PHP SDK to connect to CallRail using RESTKit",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Alex Boyce",
      "email": "curiosity26@gmail.com"
    }
